<div class="row" id="countdown">
    <div class="col-lg-12 text-center">
        <h2> 군 제대 </h2>
        <div class="countdown" id="clock"> </div>
    </div>
</div>

<script type="text/javascript">

    $('#clock').countdown('2018/04/18').on('update.countdown', function(event) {
       var $this = $(this).html(event.strftime(''
         + '<span id="count">%-w</span> week%!w '
         + '<span id="count">%-d</span> day%!d '
         + '<span id="count">%H</span> hr '
         + '<span id="count">%M</span> min '
         + '<span id="count">%S</span> sec'));
     });
